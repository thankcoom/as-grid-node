{
    "service": {
        "name": "as-grid-node",
        "dockerfile": "Dockerfile",
        "env": {
            "AUTH_SERVER_URL": {
                "description": "Official API server URL (fixed)",
                "default": "https://louisasgrid-api.zeabur.app",
                "required": true
            },
            "USER_ID": {
                "description": "Your user ID from the official website Deploy page",
                "required": true
            },
            "NODE_SECRET": {
                "description": "Node secret key (customizable, remember it)",
                "random": true
            }
        }
    }
}